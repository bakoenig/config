 
""""""""""""""
" Appearance "
""""""""""""""
colorscheme solarized-dark
"colorscheme lightgrey
set visualbell
set hlfind
set! browser.tabs.closeWindowWithLastTab=false
 
"""""""""""
" Options "
"""""""""""
set autocomplete=
set history=20
set messages=50
set defsearch=g
set hintkeys=asdfghjkl
set hintinputs=
set maxitems=15
set sanitizeshutdown=downloads,marks
set sanitizetimespan=1w
set showtabline=multitab
 
"""""""""""""""
" Keymappings "
"""""""""""""""
map -builtin j 9j
map -builtin k 9k
map J :tabNext<cr>
map K :tabnext<cr>
